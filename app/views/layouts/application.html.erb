<!DOCTYPE html>
<html>
<head>
  <title>SharePortalApp</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

</head>
<body>
<div id="wrapper">
  <!-- Sidebar -->
  <div id="sidebar-wrapper">
    <ul class="sidebar-nav">
      <li class="sidebar-brand">
        <a href="#">
          Trade Portal
        </a>
      </li>
      <li>
        <%= link_to 'Companies' , companies_path %>
      </li>
      <li>
        <%= link_to 'Departments' , departments_path %>
      </li>
      <li>
        <%= link_to 'Categories' , categories_path %>
      </li>
      <li>
        <%= link_to 'Technologies' ,technologies_path %>
      </li>
      <li>
        <%= link_to 'Employees' ,people_path %>
      </li>
    </ul>
  </div>
  <!-- /#sidebar-wrapper -->
  <!-- Page Content -->
  <div id="page-content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <%= yield %>
          <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Click for Menu</a>
        </div>
      </div>
    </div>
  </div>
  <!-- /#page-content-wrapper -->

</div>
<!-- /#wrapper -->

</body>

</html>